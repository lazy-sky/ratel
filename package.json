{
  "name": "ratel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env": "yarn react-env --env APP_ENV",
    "dev": "APP_ENV=dev yarn react-env --env APP_ENV -- next dev",
    "build": "next build",
    "start": "yarn react-env --env APP_ENV -- next start",
    "lint": "next lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --config video=false",
    "cy:component": "yarn cy:run --component"
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "bluebird": "^3.7.2",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^7.0.2",
    "@cypress/webpack-dev-server": "^3.3.0",
    "@next/eslint-plugin-next": "^13.2.4",
    "@testing-library/cypress": "^9.0.0",
    "@types/bluebird": "^3",
    "@types/cypress": "^1.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.2",
    "@types/prettier": "^2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.7.0",
    "cypress-mochawesome-reporter": "^3.3.0",
    "eslint": "8.35.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-dev-server": "^4.11.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "cobertura",
      "text"
    ],
    "report-dir": "cypress/reports/coverage",
    "all": true,
    "include": [
      "src/components/**/*.cy.ts",
      "src/components/**/*.cy.tsx"
    ],
    "exclude": []
  }
}
